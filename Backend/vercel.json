{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET,HEAD,PUT,PATCH,POST,DELETE"],
      "headers": {
        "Access-Control-Allow-Origin": "https://study-buddy-ai-uok8.vercel.app/"
      }
    }
  ]
}
